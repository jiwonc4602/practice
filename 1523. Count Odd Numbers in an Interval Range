class Solution(object):
    def countOdds(self, low, high):
        """
        :type low: int
        :type high: int
        :rtype: int
        """
        count = 0
        total = (high - low) + 1
        
        if total % 2 == 0:
            count += (total // 2)
        else:
            if low % 2 != 0:
                count += (total // 2) + 1
            else:
                count += total // 2
        return count
